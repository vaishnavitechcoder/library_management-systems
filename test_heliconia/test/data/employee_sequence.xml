<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">
    <record id="seq_Employee" model="ir.sequence">
        <field name="name">Employee Sequence</field>
        <field name="code">hr.employee</field>
        <field name="prefix">EMP</field>
        <field name="padding">4</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="ref_seq" model="ir.sequence">
        <field name="name">invoicing</field>
        <field name="code">account.move</field>
        <field name="prefix">JE/%(year)s/%(month)s</field>
        <field name="padding">2</field>
        <field name="company_id" eval="False"/>
    </record>
</odoo>